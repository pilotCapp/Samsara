{
  "expo": {
    "name": "samsara",
    "slug": "samsara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/samsara_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/samsara_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kristsey.samsara"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/samsara_icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/samsara_icon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
